      <SpeakerId>7</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>49532</PointerOffset>
      <JapaneseText>では出発しましょう</JapaneseText>
