      <SpeakerId>7</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>49368</PointerOffset>
      <JapaneseText>はい
いいえ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>25</Id>
