      <EnglishText/>
      <Notes/>
      <Id>21</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>3</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>67512</PointerOffset>
      <JapaneseText>ふっふ〜ん！
