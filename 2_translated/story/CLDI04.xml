      <EnglishText/>
      <Notes/>
      <Id>17</Id>
      <BubbleId>2</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>66644</PointerOffset>
      <JapaneseText>し、失礼な！
