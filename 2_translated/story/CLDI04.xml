      <Id>13</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>65600</PointerOffset>
      <JapaneseText>はっ、&lt;Beryl&gt;!?
