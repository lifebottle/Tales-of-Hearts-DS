      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>20</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>62544</PointerOffset>
