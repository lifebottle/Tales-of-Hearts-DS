      <SpeakerId>3</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>59068</PointerOffset>
      <VoiceId>S241412</VoiceId>
      <JapaneseText>リチア様、危険です！</JapaneseText>
