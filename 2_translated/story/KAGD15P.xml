      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>42</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>86500</PointerOffset>
      <VoiceId>S241609</VoiceId>
      <JapaneseText>クリードを助ける気か？
