    <Entry>
      <PointerOffset>46664</PointerOffset>
      <JapaneseText>呼ばれても、呼ばれなくても
商品抱えて世界の果てまで
押しかける！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>2</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>46664</PointerOffset>
      <JapaneseText>それが『商人道』ってもんさぁ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <BubbleId>2</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>2</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>46664</PointerOffset>
      <JapaneseText></JapaneseText>
      <EnglishText/>
