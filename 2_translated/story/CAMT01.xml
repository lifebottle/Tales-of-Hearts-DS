    <Entry>
      <PointerOffset>43704</PointerOffset>
      <JapaneseText>ぐ……ぐぅぅ……ッ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43832</PointerOffset>
      <JapaneseText>この人、デスピル病に……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43960</PointerOffset>
      <JapaneseText>体中が傷だらけじゃねぇか！
おい、おっさん！
何があったんだ!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>3</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44088</PointerOffset>
      <JapaneseText>…………ッ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44340</PointerOffset>
      <JapaneseText>無理に喋らなくていいよ。
みんな、スピルリンクしよう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44716</PointerOffset>
      <JapaneseText>うん、油断しないで。
ゼロムがいるのは間違いないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>4</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44844</PointerOffset>
      <JapaneseText>……ア……イ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>45160</PointerOffset>
      <JapaneseText>……？
何か言おうとしてる!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>45376</PointerOffset>
      <JapaneseText>……ア……イ……
フ……リ……ード</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>45504</PointerOffset>
      <JapaneseText>アイ……フリード？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>47520</PointerOffset>
      <JapaneseText>俺が言った通りに
船乗りに伝えればいい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>47520</PointerOffset>
      <JapaneseText>勇敢な船乗りであれば
君たちを『虹の海域』に
連れて行ってくれるだろう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <BubbleId>2</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>47520</PointerOffset>
      <JapaneseText>それから……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <BubbleId>3</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>47520</PointerOffset>
      <JapaneseText>俺たちを襲ったのは
おそらく『宝の番人』だ。
……気をつけろよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <BubbleId>4</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
