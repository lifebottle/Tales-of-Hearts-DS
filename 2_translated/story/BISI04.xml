      <Notes/>
      <Id>7</Id>
      <BubbleId>2</BubbleId>
      <SpeakerId>3</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>57068</PointerOffset>
      <JapaneseText>あれ？
