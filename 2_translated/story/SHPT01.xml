      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>53328</PointerOffset>
      <JapaneseText>何よりインカローズという
