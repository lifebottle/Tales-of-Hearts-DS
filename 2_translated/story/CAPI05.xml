      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>54932</PointerOffset>
      <JapaneseText>ちょっと待てよ。
