    <Entry>
      <PointerOffset>49828</PointerOffset>
      <JapaneseText>……ああ、わたくしは
やっとわかりました</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>49828</PointerOffset>
      <JapaneseText>世界は、己のスピリアの在り方
ひとつで、どのようにも
変える事ができるのだと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>49828</PointerOffset>
      <JapaneseText>あきらめなければいけない事など
何ひとつないのだと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <BubbleId>3</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>49828</PointerOffset>
      <JapaneseText>カルとあなた方が
気付かせてくれたのです</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <SubId>2</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>49828</PointerOffset>
      <JapaneseText>人の胸の奥に輝く
こんなにもあたたかい
スピリアの光に——</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <BubbleId>2</BubbleId>
      <SubId>2</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>49828</PointerOffset>
      <JapaneseText>ありがとう……
本当にありがとう！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
