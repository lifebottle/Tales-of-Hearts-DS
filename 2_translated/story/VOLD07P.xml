      <SpeakerId>2</SpeakerId>
      <Id>26</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>113356</PointerOffset>
      <VoiceId>S211228</VoiceId>
