      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>27</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>113904</PointerOffset>
      <VoiceId>S211229</VoiceId>
