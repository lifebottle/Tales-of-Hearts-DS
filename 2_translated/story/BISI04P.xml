      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>11</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>57592</PointerOffset>
