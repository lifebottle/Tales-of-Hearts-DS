    <Entry>
      <PointerOffset>72172</PointerOffset>
      <JapaneseText>だからさ、母さん。
あれを教えてよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>39</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72300</PointerOffset>
      <JapaneseText>あれ……って？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>40</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72428</PointerOffset>
      <JapaneseText>子どもの頃
歌ってくれた子守唄だよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>41</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72428</PointerOffset>
      <JapaneseText>俺も、息子に聞かせて
やりたいんだけど、歌詞の
最後の方を覚えてなくてさ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>41</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72428</PointerOffset>
      <JapaneseText>ごめんね、母さん。
あんなに歌ってもらったのに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>41</Id>
      <BubbleId>3</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72556</PointerOffset>
      <JapaneseText>この子ったら……
……うぅ……うぅ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>42</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72684</PointerOffset>
      <JapaneseText>泣かないでよ、母さん。
今から孫に会うんだ。
そんな顔じゃ孫に笑われちゃうよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>43</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72684</PointerOffset>
      <JapaneseText>さあ、行こう</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>43</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72944</PointerOffset>
      <JapaneseText>ありがとう。あんたたちが
背中を押してくれなかったら
一生後悔してたわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>44</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>73248</PointerOffset>
      <JapaneseText>へっ、気にすんな。
酔いどめの薬のお礼さ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>45</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>73376</PointerOffset>
      <JapaneseText>よかったね、おばさん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>46</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>73504</PointerOffset>
      <JapaneseText>もう『おばさん』じゃないよ。
これからは
『おばあちゃん』さ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>47</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>73880</PointerOffset>
      <JapaneseText>……やっぱり
家族っていいよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>48</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>74008</PointerOffset>
      <JapaneseText>そう……だな……
……ぐすっ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>49</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>74288</PointerOffset>
      <JapaneseText>お兄ちゃん、泣いてるの!?
ひょっとして……感動？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>50</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>74416</PointerOffset>
      <JapaneseText>ち、ちげぇよ！
ちょっと想像しちまったんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>51</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>74416</PointerOffset>
      <JapaneseText>いつかお前に
『今日からは、おじさんね∀　』
な〜んて言われた時の事をよぉ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>51</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>74696</PointerOffset>
      <JapaneseText>うおぉぉぉ！
やっぱ、お前は嫁にやらねぇぞ！
兄ちゃんは断固阻止だッ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>10</SpeakerId>
      <Id>52</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>74960</PointerOffset>
      <JapaneseText>どうしてそうなるのよッ!?
家族はいいって言った
ばっかりなのにぃ！</JapaneseText>
