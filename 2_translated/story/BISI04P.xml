      <SpeakerId>1</SpeakerId>
      <Id>11</Id>
      <BubbleId>3</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>57776</PointerOffset>
