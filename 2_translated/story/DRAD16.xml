    <Entry>
      <PointerOffset>56956</PointerOffset>
      <JapaneseText>ハシゴ発見！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>33</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>56956</PointerOffset>
      <JapaneseText>くっ……
でも、高すぎて届かないや</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>33</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>56972</PointerOffset>
      <JapaneseText>……上に登るハシゴ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>34</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>56972</PointerOffset>
      <JapaneseText>……けど、手が届かないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>34</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>56988</PointerOffset>
      <JapaneseText>収納式のハシゴか……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>35</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>56988</PointerOffset>
      <JapaneseText>くそっ……高すぎて
引き降ろせそうもねぇな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>35</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>57004</PointerOffset>
      <JapaneseText>収納式のハシゴみたいだけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>36</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>57004</PointerOffset>
      <JapaneseText>ちょっと届きそうもない高さね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>36</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>57020</PointerOffset>
      <JapaneseText>あっ！
ハシゴみ〜っけ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>37</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>57020</PointerOffset>
      <JapaneseText>……けど、届きそうもない
ビミョーな高さ……
あうぅ〜、悩ましいぃ〜〜〜っ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>37</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>57036</PointerOffset>
      <JapaneseText>収納式のハシゴ型構造物を確認</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>38</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>57036</PointerOffset>
      <JapaneseText>……だが、あの位置では
自分のソーマ・ヴェックスでも
引きよせる事は不可能だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>38</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
