      <EnglishText/>
      <Notes/>
      <Id>101</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>9</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>107956</PointerOffset>
      <JapaneseText>……気絶しているだけで
命に別状はなさそうだ</JapaneseText>
      <EnglishText/>
