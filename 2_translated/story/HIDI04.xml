      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>66808</PointerOffset>
      <JapaneseText>里の平穏こそ、あなたたちの母
