    <Entry>
      <PointerOffset>66176</PointerOffset>
      <JapaneseText>くそっ、橋が壊れてる!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>66176</PointerOffset>
      <JapaneseText>……けど
これくらいなら、道具があれば
直せるかもしれないな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>37</Id>
      <BubbleId>2</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>66192</PointerOffset>
      <JapaneseText>ああっ！
橋が壊れちゃってる!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>66192</PointerOffset>
      <JapaneseText>……でも、道具さえあれば
なんとか修理できそうだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <BubbleId>2</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
