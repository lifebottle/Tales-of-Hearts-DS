    <Entry>
      <PointerOffset>70504</PointerOffset>
      <JapaneseText>ふぅ、危なかった〜！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>70504</PointerOffset>
      <JapaneseText>もうちょっとで、魔物に
やられるとこだったよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>70684</PointerOffset>
      <JapaneseText>捕まえた。
もう逃げないでね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>70864</PointerOffset>
      <JapaneseText>へへ、捕まえたぜ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>70864</PointerOffset>
      <JapaneseText>ヘビ野郎に食われなくて
よかったな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>4</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>71044</PointerOffset>
      <JapaneseText>捕ま〜え〜た〜っ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>71044</PointerOffset>
      <JapaneseText>まったく、世話の焼ける
茶色いシロだなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>5</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>71224</PointerOffset>
      <JapaneseText>は〜い、もう逃げないでね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>71224</PointerOffset>
      <JapaneseText>……あん∀
そんなとこナメちゃだめよ〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>6</SpeakerId>
      <Id>6</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>71404</PointerOffset>
      <JapaneseText>『茶色いイヌ＝シロ』捕獲完了</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>7</SpeakerId>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>71404</PointerOffset>
      <JapaneseText>…………警告。
自分の顔をナメるのを停止せよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>7</SpeakerId>
      <Id>7</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72112</PointerOffset>
      <JapaneseText>いた！
茶色いシロだッ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72292</PointerOffset>
      <JapaneseText>見つけた……シロだよ……</JapaneseText>
