      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72476</PointerOffset>
      <JapaneseText>よっしゃあ、見つけたぜ！</JapaneseText>
