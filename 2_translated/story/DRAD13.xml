      <SpeakerId>4</SpeakerId>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72656</PointerOffset>
      <JapaneseText>いたぁっ！
