      <EnglishText/>
      <Notes/>
      <SpeakerId>5</SpeakerId>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>72836</PointerOffset>
