      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <BubbleId>2</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>59476</PointerOffset>
