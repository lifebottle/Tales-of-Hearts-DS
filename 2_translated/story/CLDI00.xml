      <EnglishText/>
      <Notes/>
      <SpeakerId>7</SpeakerId>
      <Id>15</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
