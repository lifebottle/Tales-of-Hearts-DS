      <EnglishText/>
      <Notes/>
      <SpeakerId>7</SpeakerId>
      <Id>13</Id>
      <BubbleId>2</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>64404</PointerOffset>
      <JapaneseText>毛あし短ッ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>64644</PointerOffset>
      <JapaneseText>そうなんだ。
