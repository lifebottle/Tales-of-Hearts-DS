    <Entry>
      <PointerOffset>43008</PointerOffset>
      <VoiceId>S242001</VoiceId>
      <JapaneseText>『いばらの森』……
まるでエメラルドの星みたいだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43136</PointerOffset>
      <VoiceId>S242002</VoiceId>
      <JapaneseText>うん……
リチアの髪と同じ色だね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43344</PointerOffset>
      <VoiceId>S242003</VoiceId>
      <JapaneseText>&lt;Hisui&gt;……
もう我慢しないで
泣いてもいいんだよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>3</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44076</PointerOffset>
      <VoiceId>S242004</VoiceId>
      <JapaneseText>泣くかッ！
あの輝きはリチアが
安らかに夢を見てる証なんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>4</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44076</PointerOffset>
      <VoiceId>S242005</VoiceId>
      <JapaneseText>……だから、泣いたりするかよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <BubbleId>2</BubbleId>
      <SpeakerId>4</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44512</PointerOffset>
      <VoiceId>S242006</VoiceId>
      <JapaneseText>これで……
全部終わったのかしら？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>45292</PointerOffset>
      <VoiceId>S242007</VoiceId>
      <JapaneseText>ううん、終わりじゃないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>45920</PointerOffset>
      <VoiceId>S242008</VoiceId>
      <JapaneseText>そうだよ！
つながる想いに
終わりなんてないんだから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>46184</PointerOffset>
      <VoiceId>S242009</VoiceId>
      <JapaneseText>ぐふふ……
けど、始まりはあるよねぇ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>3</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>46544</PointerOffset>
      <VoiceId>S242010</VoiceId>
      <JapaneseText>そうね！
まず最初のスピリアをつなげる
努力をしなきゃ……ね♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>47424</PointerOffset>
      <VoiceId>S242011</VoiceId>
      <JapaneseText>そ、そんなの……
言わなくたって
もうわかってるよね……!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>10</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>48104</PointerOffset>
      <VoiceId>S242012</VoiceId>
      <JapaneseText>それでも…………
ちゃんと言って欲しい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>48876</PointerOffset>
      <VoiceId>S242013</VoiceId>
      <JapaneseText>しっかりしやがれ、&lt;Shing&gt;！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>4</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>49924</PointerOffset>
      <VoiceId>S242014</VoiceId>
      <JapaneseText>…………え、えっと……
オ、オ、オレは…………</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>13</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>50140</PointerOffset>
      <VoiceId>S242015</VoiceId>
      <JapaneseText>…………&lt;Kohaku&gt;がぁッ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>50444</PointerOffset>
      <VoiceId>S242016</VoiceId>
      <JapaneseText>……くすっ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>50572</PointerOffset>
      <VoiceId>S242017</VoiceId>
      <JapaneseText>あははは！
&lt;Shing&gt;、変な声〜〜〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>51040</PointerOffset>
      <VoiceId>S242018</VoiceId>
      <JapaneseText>わ、笑うなよ……
たったひと言だけど
すごく難しいんだよ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>17</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>51344</PointerOffset>
      <VoiceId>S242019</VoiceId>
      <JapaneseText>うん、難しいね……
すごく簡単なのに、すごく難しい。
だけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>18</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>51636</PointerOffset>
      <VoiceId>S242020</VoiceId>
      <JapaneseText>……ああ。
けど、ほんの少しの
勇気があれば……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>19</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>51764</PointerOffset>
