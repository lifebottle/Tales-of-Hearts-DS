      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>75652</PointerOffset>
      <JapaneseText>ただ優しいだけじゃない。
