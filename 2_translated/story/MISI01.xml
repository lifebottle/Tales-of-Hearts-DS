      <EnglishText/>
      <Notes/>
      <Id>42</Id>
      <BubbleId>3</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>75896</PointerOffset>
      <JapaneseText>わ、わたしなんて
まだまだです……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>43</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>76024</PointerOffset>
      <JapaneseText>へっ、当然だぜ。
