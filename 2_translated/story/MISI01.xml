      <Id>52</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>5</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>78288</PointerOffset>
