      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43724</PointerOffset>
      <VoiceId>S241905</VoiceId>
      <JapaneseText>『いばらの森』の　夢見姫
