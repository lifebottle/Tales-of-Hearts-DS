      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>29</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
