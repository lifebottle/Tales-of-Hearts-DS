      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>16920</PointerOffset>
      <JapaneseText>あのちいせぇ体で試練に
