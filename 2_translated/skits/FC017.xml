    <Entry>
      <PointerOffset>5724</PointerOffset>
      <VoiceId>C1701</VoiceId>
      <JapaneseText>ところで、&lt;Shing&gt;。
『マリンさんが一番美人』発言に
ついて説明してくれないかしら？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>1</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>6068</PointerOffset>
      <VoiceId>C1702</VoiceId>
      <JapaneseText>説明も何も
オレが知ってる女の人で
マリンさんが一番美人って事だよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>6496</PointerOffset>
      <VoiceId>C1703</VoiceId>
      <JapaneseText>……それで
一番かわいいのは&lt;Kohaku&gt;で
一番ステキなのは&lt;Ines&gt;だ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>7160</PointerOffset>
      <VoiceId>C1704</VoiceId>
      <JapaneseText>一番……かわいい……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>3</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>7412</PointerOffset>
      <VoiceId>C1705</VoiceId>
      <JapaneseText>まぁ、一番ステキなんて……
……よく言われるのよねぇ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>1</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>7872</PointerOffset>
      <VoiceId>C1706</VoiceId>
      <JapaneseText>ボ、ボクは？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>4</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>8756</PointerOffset>
      <VoiceId>C1707</VoiceId>
      <JapaneseText>&lt;Beryl&gt;は
オレが今まで会った
女の人の中で一番……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>7</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>9160</PointerOffset>
      <VoiceId>C1708</VoiceId>
      <JapaneseText>大っきな筆を持ってる！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <BubbleId>1</BubbleId>
      <SpeakerId>2</SpeakerId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>9688</PointerOffset>
      <VoiceId>C1709</VoiceId>
      <JapaneseText>むっかぁ〜〜〜っ！
&lt;Shing&gt;は一番乙女心が
わかんない奴だよっ!!</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
