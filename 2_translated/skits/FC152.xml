      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>14</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>15216</PointerOffset>
      <JapaneseText>…………ええ、全然平気よ♪</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>15</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>15440</PointerOffset>
