      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>21</Id>
      <BubbleId>1</BubbleId>
      <SubId>1</SubId>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>20508</PointerOffset>
