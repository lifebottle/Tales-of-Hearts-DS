    <Entry>
      <PointerOffset>8904</PointerOffset>
      <JapaneseText>&lt;Shing&gt;ぅ〜……
ほんとにペリドットなんかを
仲間にしていいのぉ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>1</Id>
      <StructId>1</StructId>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>4</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>8976</PointerOffset>
      <JapaneseText>ちょっと口は悪いけど
悪い人じゃないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>2</Id>
      <StructId>2</StructId>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>1</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>9304</PointerOffset>
      <JapaneseText>で〜も、&lt;Kohaku&gt;を
人質にしたヤツだよぉ？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>3</Id>
      <StructId>3</StructId>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>4</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>9468</PointerOffset>
      <JapaneseText>う〜ん、ペリドットは……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>4</Id>
      <StructId>4</StructId>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>0</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>9560</PointerOffset>
      <JapaneseText>ペリドットは……
・よくないな！
・いい奴だよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>5</Id>
      <StructId>5</StructId>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>0</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>9700</PointerOffset>
      <JapaneseText>よくないな！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>6</Id>
      <StructId>6</StructId>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>0</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>10080</PointerOffset>
      <JapaneseText>&lt;Shing&gt;!?</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>7</Id>
      <StructId>7</StructId>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>1</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>10244</PointerOffset>
      <JapaneseText>あんな短いスカートで
キックしたり、宙返りしたり……
教育的に、すっごくよくないよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>8</Id>
      <StructId>8</StructId>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>0</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>10944</PointerOffset>
      <JapaneseText>……間違いなく見てるね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <StructId>9</StructId>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>4</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>11068</PointerOffset>
      <JapaneseText>&lt;Shing&gt;……サイテー……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>10</Id>
      <StructId>10</StructId>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>1</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>11192</PointerOffset>
      <JapaneseText>い、いや違うよ！
見たんじゃなくて
見えちゃうからよくないって……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>11</Id>
      <StructId>11</StructId>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>0</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>12380</PointerOffset>
      <JapaneseText>いい奴だよ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>12</Id>
      <StructId>12</StructId>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>0</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>12708</PointerOffset>
      <JapaneseText>うん、そうだよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>13</Id>
      <StructId>13</StructId>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>1</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>13036</PointerOffset>
      <JapaneseText>わたしを捕まえた時も
『涙をふきな』って
ハンカチをくれたんだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>14</Id>
      <StructId>14</StructId>
      <SpeakerId>2</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>1</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>13160</PointerOffset>
      <JapaneseText>……その前に
思いっきり蹴ってたけどね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>15</Id>
      <StructId>15</StructId>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>4</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>13284</PointerOffset>
      <JapaneseText>それを言ったら&lt;Beryl&gt;も
最初はオレを
ボコボコにしたじゃないか？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>16</Id>
      <StructId>16</StructId>
      <SpeakerId>3</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>0</UnknownPointer2>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>13528</PointerOffset>
      <JapaneseText>ボ、ボクはいいんだよ！
付きあえば、すぐに
いい子だってわかるからね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>17</Id>
      <StructId>17</StructId>
      <SpeakerId>1</SpeakerId>
      <UnknownPointer1>0</UnknownPointer1>
      <UnknownPointer2>4</UnknownPointer2>
